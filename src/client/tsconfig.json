{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "sourceMap": true,
        "outDir": "../../out/client",
        "jsx": "react",
        "skipLibCheck": true,
        "types": [
        ],
        "baseUrl": ".",
        "paths": {
            "$lib/*": ["lib/*"]
        },
        "lib": [
            "dom",
            "es2015"
        ]    },
    "include": [
        "../common/config.d.ts",
        "./**/.config.ts",
        "./**/*"
    ],
    "exclude": [
        "../../node_modules",
        "../server",
        "../tasks"
    ]
}
